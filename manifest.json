{
  "manifest_version": 2,
  "name": "voicetext sample app",
  "description": "VoiceTextのサンプルアプリです。",
  "version": "0.1.0",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },

  "permissions": [
    "alwaysOnTopWindows",
    "storage",
    "https://api.voicetext.jp/*"
  ]
}
